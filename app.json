{
    "expo": {
        "name": "vjti",
        "slug": "vjti",
        "platforms": [
            "ios",
            "android",
            "web"
        ],
        "android": {
            "permissions": [
                "ACCESS_FINE_LOCATION",
                "ACCESS_COARSE_LOCATION",
                "READ_EXTERNAL_STORAGE",
                "WRITE_EXTERNAL_STORAGE"
            ]
        },
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "backgroundColor": "#000000",

            "image": "./assets/res/mipmap-xxxhdpi/splashscreen.png",
            "resizeMode": "contain"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        }
    }
}